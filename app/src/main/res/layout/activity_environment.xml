<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C929AF7B"
    tools:context=".Environment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView14"
                android:layout_width="247dp"
                android:layout_height="32dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:background="#0220A668"
                android:text="@string/your_environment"
                android:textColor="#040303"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="20dp"
                android:text="@string/have_you_got_a_sedentary_work"
                android:textColor="#050505"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView14" />

            <RadioGroup
                android:id="@+id/radioGroup9"
                android:layout_width="91dp"
                android:layout_height="82dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="4dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView15">

                <RadioButton
                    android:id="@+id/radioButton27"
                    android:layout_width="73dp"
                    android:layout_height="36dp"
                    android:text="@string/yes"
                    tools:ignore="TouchTargetSizeCheck,DuplicateSpeakableTextCheck" />

                <RadioButton
                    android:id="@+id/radioButton28"
                    android:layout_width="75dp"
                    android:layout_height="48dp"
                    android:text="@string/no"
                    tools:ignore="TouchTargetSizeCheck,TextContrastCheck,DuplicateSpeakableTextCheck" />
            </RadioGroup>

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="24dp"
                android:text="@string/do_you_do_sport_regularly"
                android:textColor="#050505"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/radioGroup9" />

            <RadioGroup
                android:id="@+id/radioGroup10"
                android:layout_width="111dp"
                android:layout_height="82dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="8dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView16">

                <RadioButton
                    android:id="@+id/radioButton29"
                    android:layout_width="92dp"
                    android:layout_height="48dp"
                    android:text="@string/yes" />

                <RadioButton
                    android:id="@+id/radioButton30"
                    android:layout_width="91dp"
                    android:layout_height="48dp"
                    android:text="@string/no"
                    tools:ignore="TextContrastCheck,TouchTargetSizeCheck" />
            </RadioGroup>

            <TextView
                android:id="@+id/textView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="20dp"
                android:text="@string/how_many_times_a_week"
                android:textColor="#040202"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/radioGroup10" />

            <EditText
                android:id="@+id/editTextNumber1"
                android:layout_width="111dp"
                android:layout_height="48dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="3dp"
                android:ems="10"
                android:inputType="number"
                app:layout_constraintEnd_toEndOf="@+id/radioGroup10"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView17"
                tools:ignore="SpeakableTextPresentCheck" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="12dp"
                android:text="@string/do_you_smoke"
                android:textColor="#040202"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextNumber1" />

            <RadioGroup
                android:id="@+id/radioGroup11"
                android:layout_width="100dp"
                android:layout_height="83dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="12dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView18">

                <RadioButton
                    android:id="@+id/radioButton26"
                    android:layout_width="81dp"
                    android:layout_height="37dp"
                    android:text="@string/yes"
                    tools:ignore="TouchTargetSizeCheck" />

                <RadioButton
                    android:id="@+id/radioButton31"
                    android:layout_width="78dp"
                    android:layout_height="48dp"
                    android:text="@string/no"
                    tools:ignore="TextContrastCheck,TouchTargetSizeCheck" />
            </RadioGroup>

            <Button
                android:id="@+id/goResult"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="goToResult"
                android:text="@string/next"
                app:layout_constraintBottom_toBottomOf="@+id/perviousSuivi"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/perviousSuivi"
                app:layout_constraintTop_toTopOf="@+id/perviousSuivi"
                tools:ignore="TextContrastCheck" />

            <Button
                android:id="@+id/perviousSuivi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="68dp"
                android:layout_marginTop="64dp"
                android:onClick="goBackSuivi"
                android:text="@string/previous"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/radioGroup11"
                tools:ignore="TextContrastCheck" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>