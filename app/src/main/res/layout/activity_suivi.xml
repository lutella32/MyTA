<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Suivi">


        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="205dp"
            android:layout_height="61dp"
            android:layout_marginTop="52dp"
            android:contentDescription="mon_suivi_cardiaque"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/suivi"
            tools:ignore="ImageContrastCheck" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="16dp"
            android:text="Have you ever talked about your heart with your doctor ?"
            app:layout_constraintBottom_toTopOf="@+id/textView8"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView4"
            app:layout_constraintVertical_bias="0.073"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="92dp"
            android:text="Have you ever did a heart checkpoint ?"
            app:layout_constraintBottom_toTopOf="@+id/textView10"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="128dp"
            android:text="Have you ever had an appointment with a cardiologist ?"
            app:layout_constraintBottom_toTopOf="@+id/finish"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.238"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="TextContrastCheck" />

        <Button
            android:id="@+id/finish"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="84dp"
            android:layout_marginBottom="124dp"
            android:onClick="gofinish"
            android:text="next"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="TextContrastCheck" />

        <Button
            android:id="@+id/previousHeart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginBottom="124dp"
            android:onClick="goBackHeart"
            android:text="previous"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/finish"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="TextContrastCheck" />

        <RadioGroup
            android:layout_width="175dp"
            android:layout_height="52dp"
            android:layout_marginTop="32dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.466"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView10">

                <RadioButton
                    android:id="@+id/radioButton24"
                    android:layout_width="84dp"
                    android:layout_height="53dp"
                    android:buttonTint="#040404"
                    android:text="Yes"
                    android:textColor="#050505" />

                <RadioButton
                    android:id="@+id/radioButton25"
                    android:layout_width="94dp"
                    android:layout_height="match_parent"
                    android:buttonTint="#040404"
                    android:text="No" />
        </RadioGroup>

        <RadioGroup
            android:layout_width="158dp"
            android:layout_height="56dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@+id/textView10"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.434"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView8"
            app:layout_constraintVertical_bias="0.424">

                <RadioButton
                    android:id="@+id/radioButton14"
                    android:layout_width="73dp"
                    android:layout_height="match_parent"
                    android:buttonTint="#040404"
                    android:text="Yes" />

                <RadioButton
                    android:id="@+id/radioButton15"
                    android:layout_width="87dp"
                    android:layout_height="match_parent"
                    android:buttonTint="#040404"
                    android:text="No" />
        </RadioGroup>

        <RadioGroup
            android:layout_width="166dp"
            android:layout_height="53dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@+id/textView8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.416"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:layout_constraintVertical_bias="0.454">

                <RadioButton
                    android:id="@+id/radioButton11"
                    android:layout_width="77dp"
                    android:layout_height="match_parent"
                    android:buttonTint="#040404"
                    android:text="Yes"
                    tools:ignore="DuplicateSpeakableTextCheck" />

                <RadioButton
                    android:id="@+id/radioButton12"
                    android:layout_width="92dp"
                    android:layout_height="match_parent"
                    android:buttonTint="#040404"
                    android:text="No"
                    tools:ignore="DuplicateSpeakableTextCheck" />
        </RadioGroup>

</androidx.constraintlayout.widget.ConstraintLayout>